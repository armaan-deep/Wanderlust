<!-- Sucess message -->
<% if(sucMsg&&sucMsg.length){ %>
<div
 class="alert alert-success  alert-dismissible fade show offset-3 col-6" role="alert">
    <%= sucMsg  %>
    <button 
    type="button"
     class="btn-close"
      data-bs-dismiss="alert"
       aria-label="Close">
    </button>
</div>
<% }  %>

<!-- Error message -->
<% if(errMsg&&errMsg.length){ %>
   <div
    class="alert alert-danger  alert-dismissible fade show offset-3 col-6" role="alert">
       <%= errMsg  %>
       <button 
       type="button"
        class="btn-close"
         data-bs-dismiss="alert"
          aria-label="Close">
       </button>
   </div>
   <% }  %>
   